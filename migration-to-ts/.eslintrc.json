{
    "extends": [
        "prettier",
        "plugin:prettier/recommended"
    ],
    "plugins": ["prettier", "import"],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "no-debugger": "off",
        "no-console": "error",
        "prettier/prettier": "error",
        "class-methods-use-this": "off"
    }
}
